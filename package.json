{
  "name": "cli-complete",
  "version": "0.8.0",
  "productName": "cli-complete",
  "description": "Command Line Interface Complete",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"ng serve\" \"npm run electron\"",
    "electron": "node_modules\\electron\\dist\\electron .\\src\\server\\main --inspect",
    "build": "npm run copy:client:all && ng build -prod && npm run replace:monaco-url && npm run tsc:server && npm run copy:server:all && npm run copy:config && node gen-package-json.js",
    "test": "yarn-to-come",
    "lint": "tslint --project .",
    "replace:monaco-url": "replace-in-file \"var __MONACO_PATH = '';\" \"var __MONACO_PATH = __dirname + '/';\" dist/index.html ",
    "copy:server:all": "npm run copy:server && npm run copy:getcwd && npm run copy:windows-process-tree && npm run copy:rtmodules",
    "copy:config": "del dist\\server\\config.js && ren dist\\server\\config.prod.js config.js",
    "copy:rtmodules": "cpx electron-rt-dependencies/** dist/server",
    "copy:windows-process-tree": "cpx src/server/windows-process-tree/** dist/server/windows-process-tree",
    "copy:server": "cpx src/server/*.js dist/server",
    "copy:getcwd": "cpx src/server/getcwd/x64/Release/getcwd.exe dist/server/getcwd",
    "copy:client:all": "npm run copy:monaco",
    "copy:monaco": "cpx node_modules/monaco-editor/min/** assets/monaco --clean",
    "copy:monaco:dev": "cpx node_modules/monaco-editor/dev/** assets/monaco --clean",
    "tsc:server": "tsc -p src/tsconfig.server.json",
    "tsc:server:watch": "npm run tsc:server -- -w",
    "package:win": "npm run build && cross-var electron-packager dist $npm_package_name-$npm_package_version --out=packages --platform=win32 --arch=x64 --overwrite --icon=src/favicon.ico",
    "package:winx": "cross-var electron-packager dist $npm_package_name-$npm_package_version --out=packages --platform=win32 --arch=x64 --overwrite --icon=src/favicon.ico",
    "xpackage:linux": "npm run build && cross-var electron-packager dist $npm_package_name-$npm_package_version --out=packages --platform=linux --arch=all --overwrite ",
    "xpackage:osx": "npm run build && cross-var electron-packager dist $npm_package_name-$npm_package_version --out=packages --platform=darwin --arch=all --overwrite ",
    "xpackage:all": "npm run build && cross-var electron-packager dist $npm_package_name-$npm_package_version --out=packages --all --arch=all --overwrite "
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "core-js": "^2.4.1",
    "electron": "1.7.11",
    "format-json": "^1.0.3",
    "golden-layout": "1.5.9",
    "jquery": "3.3.1",
    "monaco-editor": "^0.10.1",
    "node-pty-prebuilt": "^0.7.3",
    "rxjs": "^5.5.6",
    "xterm": "^3.2.0",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "~1.7.3",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/electron": "^1.6.10",
    "@types/jasmine": "~2.8.4",
    "@types/jasminewd2": "^2.0.3",
    "@types/jquery": "^3.3.1",
    "@types/node": "^9.3.0",
    "codelyzer": "~4.0.2",
    "concurrently": "^3.5.1",
    "cpx": "^1.5.0",
    "cross-var": "^1.1.0",
    "electron-packager": "^10.1.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.3",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.2.2",
    "replace-in-file": "^3.2.0",
    "ts-node": "^4.1.0",
    "tslint": "4.5.1",
    "typescript": "~2.6.2"
  }
}